-
  var metaTitle = 'demo title';
  var metaKeywords = 'WebGL, three.js';
  var metaDescription = 'it is my basic files for three.js demo.';
  var metaPath = '/';
  var metaOgpImg = 'share.jpg';

doctype html
html(lang="ja")
  head
    include ./_modules/_head.pug
  body
    .p-contents-summary
      h1.p-contents-summary__head
        |#{metaTitle}
      p.p-contents-summary__text
        |#{metaDescription}
      p.p-contents-summary__link-source
        a(href="https://github.com/ykob/"+repositoryName)
          |view source.
    canvas(id="canvas-webgl", class="p-canvas-webgl")
    script(src="./js/vendor.js")
    script(src="./js/main.js")
